<%= render 'layouts/errors', object: @post %>


<div class="container">
  <h1> Craft a Post! </h1>
  <%= form_for @post do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title%>
  <%= form.label :entry %>
  <%= form.text_area :entry%>
  <%= form.label :date %>
  <%= form.date_field :date%>
  <%= form.submit "Post" %>
  <% end %>
</div>
